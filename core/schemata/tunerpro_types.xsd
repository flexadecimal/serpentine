    <!-- 
    PINT TYPES
    see https://raw.githubusercontent.com/hgrecco/pint/master/pint/default_en.txt.
    concatenation of all the individual second appinfo entries, 
    e.g. 'ampere = [current] = A = amp', form definition file for a complete unit system.
    -->
    <!-- 
    UNIT TYPES - not used in tunerpro, but for python Pint unit epistemology,
    and for aliases below, e.g. `Kilometers', '@alias kilometer', eventually parsed as
    '@alias kilometer = Kilometers'
    -->
    <xs:simpleType name="unitPrefix">
        <xs:restriction base="xs:float">
            <xs:enumeration value="1e-6">
                <xs:annotation>
                    <xs:appinfo>Micro</xs:appinfo>
                    <xs:appinfo>micro- = 1e-6 = µ- = u-</xs:appinfo>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="1e-3">
                <xs:annotation>
                    <xs:appinfo>Milli</xs:appinfo>
                    <xs:appinfo>milli- = 1e-3 = m-</xs:appinfo>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="1e-2">
                <xs:annotation>
                    <xs:appinfo>Centi</xs:appinfo>
                    <xs:appinfo>centi- = 1e-2 = c-</xs:appinfo>
                </xs:annotation>
            </xs:enumeration>
            <!-- deci not used in tunerpro... skipped -->
            <xs:enumeration value="1e3">
                <xs:annotation>
                    <xs:appinfo>Kilo</xs:appinfo>
                    <xs:appinfo>kilo- = 1e3 = k-</xs:appinfo>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="1e6">
                <xs:annotation>
                    <xs:appinfo>Mega</xs:appinfo>
                    <xs:appinfo>mega- = 1e6 = M-</xs:appinfo>
                </xs:annotation>
            </xs:enumeration>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="unit">
        <xs:union memberTypes="t:null_option">
            <xs:simpleType>
                <xs:restriction base='xs:integer'>
                    <xs:enumeration value="21">
                        <xs:annotation>
                            <xs:appinfo>Air Fuel Ratio</xs:appinfo>
                            <!-- no definiton means that this is a unitless value, like a percentage -->
                        </xs:annotation>
                    </xs:enumeration>
                    <xs:enumeration value="84">
                        <xs:annotation>
                            <xs:appinfo>Seconds</xs:appinfo>
                            <xs:appinfo>second = [time] = s = sec</xs:appinfo>
                        </xs:annotation>
                    </xs:enumeration>
                    <xs:enumeration value="32">
                        <xs:annotation>
                            <xs:appinfo>Volts</xs:appinfo>
                            <xs:appinfo>
                                [electric_potential] = [energy] / [charge]
                                volt = joule / coulomb = V
                            </xs:appinfo>
                        </xs:annotation>
                    </xs:enumeration>
                    <xs:enumeration value="34">
                        <xs:annotation>
                            <xs:appinfo>Watts</xs:appinfo>
                            <xs:appinfo>
                                [power] = [energy] / [time]
                                watt = joule / second = W
                            </xs:appinfo>
                        </xs:annotation>
                    </xs:enumeration>
                    <xs:enumeration value="40">
                        <xs:annotation>
                            <xs:appinfo>Amps</xs:appinfo>
                            <xs:appinfo>
                                ampere = [current] = A = amp
                            </xs:appinfo>
                        </xs:annotation>
                    </xs:enumeration>
                    <xs:enumeration value="38">
                        <xs:annotation>
                            <xs:appinfo>Ohms</xs:appinfo>
                            <xs:appinfo>
                                [charge] = [current] * [time]
                                [electric_potential] = [energy] / [charge]
                                [resistance] = [electric_potential] / [current]
                                ohm = volt / ampere = Ω
                            </xs:appinfo>
                        </xs:annotation>
                    </xs:enumeration>
                    <xs:enumeration value="19">
                        <xs:annotation>
                            <xs:appinfo>Bar</xs:appinfo>
                            <xs:appinfo>
                                pascal = newton / meter ** 2 = Pa
                                bar = 1e5 * pascal
                            </xs:appinfo>
                        </xs:annotation>
                    </xs:enumeration>
                    <xs:enumeration value="45">
                        <xs:annotation>
                            <xs:appinfo>Meters</xs:appinfo>
                            <xs:appinfo>meter = [length] = m = metre</xs:appinfo>
                        </xs:annotation>
                    </xs:enumeration>
                    <xs:enumeration value="46">
                        <xs:annotation>
                            <xs:appinfo>Centimeters</xs:appinfo>
                            <xs:appinfo>@alias centimeter</xs:appinfo>
                        </xs:annotation>
                    </xs:enumeration>
                    <xs:enumeration value="94">
                        <xs:annotation>
                            <xs:appinfo>Poise</xs:appinfo>
                            <xs:appinfo>
                                [viscosity] = [pressure] * [time]
                                poise = 0.1 * Pa * second = P
                            </xs:appinfo>
                        </xs:annotation>
                    </xs:enumeration>
                    <xs:enumeration value="95">
                        <xs:annotation>
                            <xs:appinfo>Centipoise</xs:appinfo>
                            <xs:appinfo>@alias centipoise</xs:appinfo>
                        </xs:annotation>
                    </xs:enumeration>
                    <xs:enumeration value="26">
                        <xs:annotation>
                            <xs:appinfo>Count</xs:appinfo>
                            <xs:appinfo>count = []</xs:appinfo>
                        </xs:annotation>
                    </xs:enumeration>
                    <xs:enumeration value="60">
                        <xs:annotation>
                            <xs:appinfo>Cubic Centimeters</xs:appinfo>
                            <xs:appinfo>cubic_centimeter = centimeter ** 3 = cc</xs:appinfo>
                        </xs:annotation>
                    </xs:enumeration>
                    <xs:enumeration value="57">
                        <xs:annotation>
                            <xs:appinfo>Cubic Feet</xs:appinfo>
                            <xs:appinfo>cubic_foot = ft ** 3 = cu_ft = cubic_feet</xs:appinfo>
                        </xs:annotation>
                    </xs:enumeration>
                    <xs:enumeration value="58">
                        <xs:annotation>
                            <xs:appinfo>Cubic Inches</xs:appinfo>
                            <xs:appinfo>cubic_inch = in ** 3 = cu_in</xs:appinfo>
                        </xs:annotation>
                    </xs:enumeration>
                    <xs:enumeration value="59">
                        <xs:annotation>
                            <xs:appinfo>Cubic Meters</xs:appinfo>
                            <xs:appinfo>cubic_meter = meter ** 3</xs:appinfo>
                        </xs:annotation>
                    </xs:enumeration>
                    <xs:enumeration value="61">
                        <xs:annotation>
                            <xs:appinfo>Cubic Millimeters</xs:appinfo>
                            <xs:appinfo>cubic_meter = millimeter ** 3</xs:appinfo>
                        </xs:annotation>
                    </xs:enumeration>
                    <xs:enumeration value="56">
                        <xs:annotation>
                            <xs:appinfo>Cubic Yards</xs:appinfo>
                            <xs:appinfo>cubic_yard = yd ** 3 = cu_yd</xs:appinfo>
                        </xs:annotation>
                    </xs:enumeration>
                    <xs:enumeration value="81">
                        <xs:annotation>
                            <xs:appinfo>Days</xs:appinfo>
                            <xs:appinfo>day = 24 * hour = d</xs:appinfo>
                        </xs:annotation>
                    </xs:enumeration>
                    <xs:enumeration value="31">
                        <xs:annotation>
                            <xs:appinfo>Decibels</xs:appinfo>
                            <xs:appinfo>decibel = 1 ; logbase: 10; logfactor: 10 = dB</xs:appinfo>
                        </xs:annotation>
                    </xs:enumeration>
                    <xs:enumeration value="15">
                        <xs:annotation>
                            <xs:appinfo>Degrees Celsius</xs:appinfo>
                            <xs:appinfo>degree_Celsius = kelvin; offset: 273.15 = °C = celsius = degC = degreeC</xs:appinfo>
                        </xs:annotation>
                    </xs:enumeration>
                    <xs:enumeration value="14">
                        <xs:annotation>
                            <xs:appinfo>Degrees Fahrenheit</xs:appinfo>
                            <xs:appinfo>degree_Fahrenheit = 5 / 9 * kelvin; offset: 233.15 + 200 / 9 = °F = fahrenheit = degF = degreeF</xs:appinfo>
                        </xs:annotation>
                    </xs:enumeration>
                    <xs:enumeration value="16">
                        <xs:annotation>
                            <xs:appinfo>Degrees Kelvin</xs:appinfo>
                            <xs:appinfo>kelvin = [temperature]; offset: 0 = K = degK = °K = degree_Kelvin = degreeK</xs:appinfo>
                        </xs:annotation>
                    </xs:enumeration>
                    <xs:enumeration value="68">
                        <xs:annotation>
                            <xs:appinfo>Duty Cycle</xs:appinfo>
                        </xs:annotation>
                    </xs:enumeration>
                    <xs:enumeration value="8">
                        <xs:annotation>
                            <xs:appinfo>Feet Per Second</xs:appinfo>
                            <xs:appinfo>foot_per_second = foot / second = feet_per_second = fps</xs:appinfo>
                        </xs:annotation>
                    </xs:enumeration>
                    <xs:enumeration value="66">
                        <xs:annotation>
                            <xs:appinfo>Fluid Ounces</xs:appinfo>
                            <xs:appinfo>fluid_ounce = pint / 16 = floz = US_fluid_ounce = US_liquid_ounce</xs:appinfo>
                        </xs:annotation>
                    </xs:enumeration>
                    <xs:enumeration value="75">
                        <xs:annotation>
                            <xs:appinfo>Foot-Pounds</xs:appinfo>
                            <xs:appinfo>
                                [torque] = [force] * [length]
                                foot_pound = foot * force_pound = ft_lb = footpound
                            </xs:appinfo>
                        </xs:annotation>
                    </xs:enumeration>
                    <xs:enumeration value="65">
                        <xs:annotation>
                            <xs:appinfo>Gallons</xs:appinfo>
                            <xs:appinfo>gallon = 231 * cubic_inch = gal = liquid_gallon = US_liquid_gallon</xs:appinfo>
                        </xs:annotation>
                    </xs:enumeration>
                    <xs:enumeration value="91">
                        <xs:annotation>
                            <xs:appinfo>Grams</xs:appinfo>
