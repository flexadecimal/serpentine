TunerPro XDF/ADX units and data types
-->
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" targetNamespace="tunerpro" xmlns:t="tunerpro">
    <!-- COMMON TYPES -->
    <xs:simpleType name="uuid">
        <xs:restriction base="xs:string">
            <xs:pattern value="[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{12}" />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="hex_string">
        <xs:restriction base="xs:string">
            <xs:pattern value="0x[\da-fA-F]+" />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="hex_word">
        <xs:restriction base="xs:string">
            <xs:pattern value="[\da-fA-F]+" />
        </xs:restriction>
    </xs:simpleType>
    <!-- DATA TYPES -->
    <!-- tunerpro dropdown nullable, used in data/unit dropwdowns -->
    <xs:simpleType name="null_option">
        <xs:restriction base="xs:integer">
            <!-- TODO: find out what this does -->
            <xs:enumeration value="1">
                <xs:annotation>
                    <xs:appinfo>Externally Defined</xs:appinfo>
                </xs:annotation>
            </xs:enumeration>
            <!-- these all go to Python None -->
            <xs:enumeration value="0">
                <xs:annotation>
                    <xs:appinfo>None</xs:appinfo>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="2">
                <xs:annotation>
                    <xs:appinfo>Undefined</xs:appinfo>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="3">
                <xs:annotation>
                    <xs:appinfo>Unknown</xs:appinfo>
                </xs:annotation>
            </xs:enumeration>
        </xs:restriction>
    </xs:simpleType>
